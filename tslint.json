{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [false, "always-prefix"],
    "object-literal-shorthand": false,
    "quotemark": [true, "single"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "object-literal-sort-keys": false,
    "no-unused-variable": true
  },
  "rulesDirectory": []
}
